	UP_PAPE_CD VARCHAR(10) NOT NULL,
	PAPE_NM VARCHAR(100),
	PAPE_DTY_SE_CD VARCHAR(10),
	LIMT_CNT NUMERIC(2,0),
	ESNTL_YN CHAR(1),
	CD_ORDR NUMERIC(5,0),
	FILE_NM VARCHAR(200),
	FILE_PATH VARCHAR(200),
	DOWN_CNT NUMERIC(10),
	DOWN_TRGT_YN CHAR(1),
	USE_YN CHAR(1),
	RGTR_NO VARCHAR(20),
	REG_YMD DATE,
	MDFR_NO VARCHAR(20),
