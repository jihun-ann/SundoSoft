/* diag-search영역 */
.search-wrap { 
	width: 100%; 
	min-height: 300px; 
	display: flex; 
	-webkit-align-items: center; 
	align-items: center; 
	justify-content: center; 
	background: #f1f3f9; 
	border: 1px solid #abb8bc; 
	border-radius: 15px; 
	margin: 25px 0 35px; 
	flex-direction: column;
	padding: 20px;
}
.smart-wrap{
	position:relative;
	width: 800px;
	height: 600px;
	margin: 0 auto;
	overflow: hidden;
}
.toggle-wrap{
	position: absolute;
	height: 600px;
}
.form-wrap{
	width : 80%;
	margin: 0 auto;
	min-height: 300px;
	background: white;
	text-align: center;
	position: relative;
	padding: 30px; 
}
.search-label{
	font-size:25px;
	font-weight: bold;
	
}
.searchVal{
	display: block;
	width : 500px;
	height: 30px;
	margin: 0 auto;
	color:grey;
}
.checkbox-wrap{
	margin: 0 auto;
	width:780px;
	height: auto;
	text-align: left;
	display: flex;
	flex-wrap: wrap;
}
.checkbox-wrap>label{
	min-width: 141px;
}
.smartsearch-font{
	width: 800px;
	font-size:20px;
	font-weight: bold;
	text-align: left;
	margin: 0 auto;
}
.font-wrap{
	width: 780px;
	font-size:18px;
	font-weight: bold;
	text-align: left;
	margin: 30px auto 10px;
}
.invt-amount-wrap{
	width:640px;
	height: 100px;
}
.smartToggleBtn{
	position:absolute;
	right: 0;
	background: none;
	border: 1px solid grey;
	border-radius: 5px;
}
.searchBtn-wrap{
	margin: 50px 0;
}
.searchBtn-wrap > input{
	width: 120px;
	background: none;
	border: 1px solid grey;
	border-radius: 5px;
}
.invt-amount-wrap > input[type="range"]{
	width:400px;
	display:none;	
}
.fakeamount{
	width: 400px;
	height: 10px;
	background: lightgrey;
	position:relative;
	border-radius: 5px;
	margin-top: 50px;
	margin-left: 50px;
}
.rangepoint{
	width: 15px;
	height: 15px;
	border: 1px solid grey;
	border-radius: 8px;
	background: white;
	position: absolute;
	top: -2.05px;
	transform: translateX(-50%);
}
.rangeleft{
	left: 0px;
	cursor: pointer;
}
.rangeright{
	left: 100%;
	cursor:pointer;
}
.fakeval{
	position: absolute;
	bottom: -20px;
	left: -10px;
}
.gvmentSpHst-wrap{
	width: 140px;
    height: 100px;
    display: flex;
    flex-direction: row;
    align-items: center;
    flex-wrap: wrap;
}



















/* diag-list영역 */
.entire-irlist-wrap { 
	width: 100%; 
	min-height: 300px; 
	display: flex; 
	-webkit-align-items: center; 
	align-items: center; 
	justify-content: center; 
	background: #f1f3f9; 
	border: 1px solid #abb8bc; 
	border-radius: 15px; 
	margin: 25px 0 35px; 
	flex-direction: column;
	padding: 20px;
}
.list-wrap{
	width: 800px;
	height: 700px;
	background: white;
}
.listCount{
	font-size: 25px;
	color: blue;
}
.type-wrap{
	width: 100%;
	height: 30px;
}
.type-wrap > input[type="button"]{
	background: none;
	border: 1px solid grey;
	border-radius: 5px;
}
.type-wrap> select{
	float: right;
}
.irList{
	width: 100%;
    height: 500px;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
    align-content: space-around;
}
.ir-simple{
	width: 240px;
	height: 220px;
}
.ir-simple-img{
	width: 230px;
	height: 120px;
	border: 1px solid black;
	border-radius: 5px;
	margin: 5px;
	text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size:14px;
    position: relative;
}
.simpleBookmarkBtn{
	position: absolute;
	top: 10px;
	right: 10px;
	color: grey;
	cursor: pointer;
	line-height: 16px;
	font-size: 16px;
}
#paging-wrap{
    width: 50%;
    height: 20px;
    margin: 0 auto;
    text-align: center;
    font-size: 20px;
    line-height: 20px;
}
#paging-wrap>span{
	margin: 0 5px;
}
#paging-wrap>a{
	margin: 0 5px;
	color: #2E68FF;
}
















