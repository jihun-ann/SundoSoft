COMMENT ON COLUMN APFS.TMP_OAUTH.OATH_ID IS 'OAUTH 식별ID';
COMMENT ON COLUMN APFS.TMP_OAUTH.EMAIL IS '이메일';
COMMENT ON COLUMN APFS.TMP_OAUTH.MOBILE IS '휴대전화번호';
COMMENT ON COLUMN APFS.TMP_OAUTH.RGTR_NO IS '등록자번호';
COMMENT ON COLUMN APFS.TMP_OAUTH.REG_YMD IS '등록일자';
COMMENT ON COLUMN APFS.TMP_OAUTH.MDFR_NO IS '수정자번호';
COMMENT ON COLUMN APFS.TMP_OAUTH.MDFCN_YMD IS '수정일자';

CREATE TABLE APFS.TMP_MAIL_HST (
	SN INTEGER NOT NULL,
	USER_NO VARCHAR(20),
	TO_ADDR VARCHAR(50),
	TO_NAME VARCHAR(40),
	FR_ADDR VARCHAR(50),
	FR_NAME VARCHAR(40),
	SUBJECT VARCHAR(200),
	CONTENT TEXT,
	TEMPLATE VARCHAR(30),
	SEND_DT DATE,
	SEND_CD VARCHAR(20),
	RGTR_NO VARCHAR(20),
	REG_YMD DATE,
	MDFR_NO VARCHAR(20),
	MDFCN_YMD DATE,
	PRIMARY KEY (SN)
);
COMMENT ON TABLE  APFS.TMP_MAIL_HST IS 'TMP_MAIL_HST';
COMMENT ON COLUMN APFS.TMP_MAIL_HST.SN IS '일련번호';
COMMENT ON COLUMN APFS.TMP_MAIL_HST.USER_NO IS '사용자번호';
