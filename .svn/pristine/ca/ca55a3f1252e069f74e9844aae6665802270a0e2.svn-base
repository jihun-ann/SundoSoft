CREATE SEQUENCE APFS.SEQ_USER START 1;
CREATE SEQUENCE APFS.SEQ_MAIL_HST START 1;
CREATE SEQUENCE APFS.SEQ_APLY_FILE START 1;
CREATE SEQUENCE APFS.SEQ_FILE START 1;

CREATE TABLE APFS.TMP_USER (
	USER_NO VARCHAR(20) NOT NULL,
	USER_ID VARCHAR(100),
	USER_NM VARCHAR(40),
	USER_CD VARCHAR(10),
	PSWD  VARCHAR(100),
	EMAIL VARCHAR(100),
	MOBILE VARCHAR(20),
	JOIN_YMD VARCHAR(8),
	WHDWL_YMD VARCHAR(8),
	PSWD_LOCK_YMD VARCHAR(8),
	PSWD_ERR_CNT SMALLINT,
	PSWD_CHG_YMD VARCHAR(8),
	PSWD_NEXT_YMD VARCHAR(8),
	LST_LGN_DT DATE,
	RGTR_NO VARCHAR(20),
	REG_YMD DATE,
	MDFR_NO VARCHAR(20),
	MDFCN_YMD DATE,
	PRIMARY KEY (USER_NO)
);
COMMENT ON TABLE  APFS.TMP_USER IS '사용자정보';
COMMENT ON COLUMN APFS.TMP_USER.USER_NO IS '사용자번호';
COMMENT ON COLUMN APFS.TMP_USER.USER_ID IS '사용자ID';
COMMENT ON COLUMN APFS.TMP_USER.USER_NM IS '사용자명';
COMMENT ON COLUMN APFS.TMP_USER.USER_CD IS '사용자가입유형';
COMMENT ON COLUMN APFS.TMP_USER.PSWD IS '비밀번호';
COMMENT ON COLUMN APFS.TMP_USER.EMAIL IS '이메일';
COMMENT ON COLUMN APFS.TMP_USER.MOBILE IS '휴대전화번호';
COMMENT ON COLUMN APFS.TMP_USER.JOIN_YMD IS '가입일자';
COMMENT ON COLUMN APFS.TMP_USER.WHDWL_YMD IS '탈퇴일자';
COMMENT ON COLUMN APFS.TMP_USER.PSWD_LOCK_YMD IS '비밀번호잠금일자';
COMMENT ON COLUMN APFS.TMP_USER.PSWD_ERR_CNT IS '비밀번호오류수';
COMMENT ON COLUMN APFS.TMP_USER.PSWD_CHG_YMD IS '비밀번호변경일자';
COMMENT ON COLUMN APFS.TMP_USER.PSWD_NEXT_YMD IS '비밀번호다음일자';
COMMENT ON COLUMN APFS.TMP_USER.LST_LGN_DT IS '마지막로그인일시';
COMMENT ON COLUMN APFS.TMP_USER.RGTR_NO IS '등록자번호';
COMMENT ON COLUMN APFS.TMP_USER.REG_YMD IS '등록일자';
COMMENT ON COLUMN APFS.TMP_USER.MDFR_NO IS '수정자번호';
COMMENT ON COLUMN APFS.TMP_USER.MDFCN_YMD IS '수정일자';

CREATE TABLE APFS.TMP_OAUTH (
	USER_NO VARCHAR(20) NOT NULL,
	OATH_CD VARCHAR(20) NOT NULL,
	OATH_ID VARCHAR(50),
	USER_NM VARCHAR(40),
	EMAIL VARCHAR(100),
	MOBILE VARCHAR(20),
	RGTR_NO VARCHAR(20),
	REG_YMD DATE,
	MDFR_NO VARCHAR(20),
	MDFCN_YMD DATE,
	PRIMARY KEY (USER_NO, OATH_CD)
);
COMMENT ON TABLE  APFS.TMP_OAUTH IS '사용자정보';
COMMENT ON COLUMN APFS.TMP_OAUTH.USER_NO IS '사용자번호';
COMMENT ON COLUMN APFS.TMP_OAUTH.OATH_CD IS 'OAUTH 서비스코드';
