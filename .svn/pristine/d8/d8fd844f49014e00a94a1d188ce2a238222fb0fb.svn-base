	line-height: 60px;
}
.printBtn{
	float: right;
	margin: 10px 0;
	background: white;
}
.modal-wrap{
	position:fixed;
	top:0;
	left:0;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.5);
	display:none;
}
.modal-cont{
	position: absolute;
	width: 600px;
	height: 150px;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
	background: white;
	border-radius: 10px;
    display: none;
    align-items: center;
    justify-content: space-around;
    flex-wrap: wrap;
}
.modal-form{
	width: 100%;
	display: flex;
    align-items: center;
    justify-content: space-evenly;
}
.modal-form > input{
	width : 220px;
}
.modal-btn{
	float: right;
    width: 80px;
    height: 26px;
    background: white;
    line-height: 20px;
    font-size: 16px;
	border-radius: 5px;
}
.editBtn{
	line-height: 22px;
	font-size: 22px;
	float: right;
	color: grey;
	cursor: pointer;
}