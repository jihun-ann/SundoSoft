	line-height: 60px;
}
.basic-info-wrap table {
    width: 100%;
    border-top: 1px solid #444444;
    border-collapse: collapse;
    margin: 0 auto;
  }
.basic-info-wrap th,.basic-info-wrap td {
    border-bottom: 1px solid #444444;
    border-left: 1px solid #444444;
    padding: 5px;
  }
.basic-info-wrap th:first-child,.basic-info-wrap td:first-child {
    border-left: none;
  }
 .tablecate{
 	width: 150px;
 }
